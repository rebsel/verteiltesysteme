package Tabellen;
/**
 * 
 * represents one person containing name + nr
 */
public class Person {
	private String name;
	private int nr;
	
	public Person(String name, int nr) {
		this.name = name;
		this.nr = nr;
	}
	
	public String getName() {
		return name;
	}
	
	public void setName(String name) {
		this.name = name;
	}
	
	public int getNr() {
		return nr;
	}
	
	public void setNr(int nr) {
		this.nr = nr;
	}
	
	

}
